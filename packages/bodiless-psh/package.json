{
  "name": "@bodiless/psh",
  "version": "0.0.58",
  "description": "Provides cli tool for initializing a bodiless project on platform.sh",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "bin": {
    "bodiless-psh-init": "./lib/bodiless-psh-init.js"
  },
  "scripts": {
    "build": "tsc --version && tsc -p ./tsconfig.json ",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "/lib",
    "/resources",
    "/bodiless.docs.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "copyfiles": "^2.1.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.19",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@types/copyfiles": "^2.1.1"
  }
}
