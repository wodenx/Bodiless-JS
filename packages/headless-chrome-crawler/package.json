  "private": true,
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "greenkeeper-lockfile-update": "greenkeeper-lockfile-update",
    "greenkeeper-lockfile-upload": "greenkeeper-lockfile-upload",
    "lint": "eslint --quiet -f codeframe . || eslint .",
