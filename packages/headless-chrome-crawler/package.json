    "content-type": "^1.0.4",
    "debug": "^4.1.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.19",
    "puppeteer": "^2.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "robots-parser": "^1.0.2"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/lodash": "^4.14.158",
    "@types/node": "^10",
    "@types/puppeteer": "^1.3.4",
    "@types/request-promise": "^4.1.41",
