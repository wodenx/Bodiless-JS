{
  "name": "@bodiless/gatsby-theme-bodiless",
  "version": "0.0.58",
  "description": "A Gatsby theme for BodilessJS",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "bin": {
    "generate-env-vars": "./bin/generate-env-vars",
    "bl-validate-logs": "./bin/bl-validate-logs",
    "bl-clear-logs": "./bin/bl-clear-logs"
  },
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "/bin",
    "dist",
    "index.js",
    "index.d.ts",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "queries.jsx",
    "Logger.js",
    "src/no-scroll-settings.json",
    "build-css.js",
    "create-node.js",
    "/doc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api --mode file",
    "clean": "rimraf -rf \"dist\" && rimraf index* && rimraf \"doc/api\""
  },
  "dependencies": {
    "@bodiless/core": "^0.0.58",
    "@bodiless/core-ui": "^0.0.58",
    "@bodiless/fclasses": "^0.0.58",
    "@bodiless/ui": "^0.0.58",
    "@types/nodegit": "^0.26.1",
    "@types/walk": "^2.3.0",
    "axios": "^0.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "find-up": "^4.1.0",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-emotion": "^2.0.6",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-postcss": "2.1.20",
    "gatsby-plugin-purgecss": "4.0.1",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-plugin-typescript": "^2.3.3",
    "gatsby-source-filesystem": "^2.0.4",
    "git-branch": "^2.0.1",
    "glob": "^7.1.6",
    "informed": "^3.9.0",
    "lodash": "^4.17.19",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-react-lite": "^1.3.1",
    "nodegit": "^0.26.4",
    "null-loader": "^3.0.0",
    "slash": "^2.0.0",
    "sleep-promise": "^8.0.1",
    "tailwindcss": "^1.0",
    "universal-cookie": "^4.0.3",
    "walk": "^2.3.14",
    "walkdir": "^0.4.1"
  },
  "peerDependencies": {
    "gatsby": "^2.0.19",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
